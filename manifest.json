{
   "background": {
      "scripts": [ "scripts/angular.min.js", "background.js"]
   },
   "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html",
      "default_title": "Make better online"
   },
   "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
   "description": "Modify request and response headers",
   "icons": {
      "128": "icon_128.png",
      "16": "icon_16.png",
      "48": "icon_48.png"
   },
   "incognito": "spanning",
   "manifest_version": 2,
   "name": "Make Better Online",
   "permissions": [ "storage", "webRequest", "webRequestBlocking", "\u003Call_urls>" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.0"
}
